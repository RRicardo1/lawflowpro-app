{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(templates|products|login|pricing|enterprise|resources|solutions)\\.html",
      "dest": "/public/$1.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}